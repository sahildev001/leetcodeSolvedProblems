Q2  Search in Rotated Sorted Array
Sol in java
class Solution {
    public int search(int[] nums, int target) {
      //  int result = -1;
      /*  for(int i = 0;i<nums.length;i++){
            if(nums[i] == target){
                result = i;
            }
        }
        return result;*/
        return searchTatget(nums,target,0,nums.length-1);
        
    }
    
    int searchTatget(int[] nums, int target,int first,int last){
          if (first > last)
            return -1;
  
        int mid = (first + last) / 2;
        if (nums[mid] == target)
            return mid;
  
     
        if (nums[first] <= nums[mid]) {
            
            if (target >= nums[first] && target <= nums[mid])
                return searchTatget(nums, target, first, mid-1);
        
            return searchTatget(nums, target,mid + 1, last);
        }
        if (target >= nums[mid] && target <= nums[last])
            return searchTatget(nums, target,mid + 1, last);
  
        return searchTatget(nums,target, first, mid - 1);
    }
}
